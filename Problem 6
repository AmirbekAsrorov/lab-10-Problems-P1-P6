#include <iostream>
#include <vector>
#include <queue>
#include <string>
using namespace std;

vector<string> findRelativeRanks(const vector<int>& score) {
    int n = score.size();
    vector<string> answer(n);

    
    priority_queue<pair<int, int>> pq;
    for (int i = 0; i < n; ++i) {
        pq.push({score[i], i});
    }

    int rank = 1;
    while (!pq.empty()) {
        auto [s, idx] = pq.top(); pq.pop();
        if (rank == 1) answer[idx] = "Gold Medal ,";
        else if (rank == 2) answer[idx] = "Silver Medal ,";
        else if (rank == 3) answer[idx] = "Bronze Medal ,";
        else answer[idx] = to_string(rank);
        rank++;
    }

    return answer;
}

int main() {
    int n;
    cout << "Enter number of athletes: ";
    cin >> n;

    vector<int> scores(n);
    cout << "Enter scores of athletes (space-separated): ";
    for (int i = 0; i < n; ++i) cin >> scores[i];

    vector<string> ranks = findRelativeRanks(scores);

    cout << "Relative ranks: ";
    for (const string& s : ranks) cout << s << " ";
    cout << "\n";

    return 0;
}
